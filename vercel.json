{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "src/frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/prabandhak",
            "dest": "api/index.py"
        },
        {
            "src": "/(styles\\.css|app\\.js|admin\\.html|admin\\.css|admin\\.js)",
            "dest": "src/frontend/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ]
}